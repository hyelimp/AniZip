<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <include
        android:id="@+id/includ02"
        layout="@layout/main_navbar"
        android:layout_width="405dp"
        android:layout_height="83dp"
        android:layout_marginBottom="435dp"
        app:layout_constraintBottom_toTopOf="@+id/Layout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/Layout1"
        android:layout_width="409dp"
        android:layout_height="0dp"
        android:layout_marginTop="76dp"
        android:descendantFocusability="blocksDescendants"
        android:fontFamily="@font/skybory"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="299dp"
            android:fontFamily="@font/skybory"
            app:srcCompat="@drawable/bak1"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="98dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/Layout3"
        android:layout_width="314dp"
        android:layout_height="65dp"
        android:descendantFocusability="blocksDescendants"
        android:fontFamily="@font/skybory"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/Layout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Layout1">


        <Button
            android:id="@+id/btn1"
            android:layout_width="222dp"
            android:layout_height="53dp"
            android:background="#023e8a"
            android:fontFamily="@font/skybory"
            android:gravity="center"
            android:text="카테고리"
            android:textColor="#fff"
            android:textSize="20dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="369dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Layout2"
        android:layout_width="402dp"
        android:layout_height="59dp"
        android:descendantFocusability="blocksDescendants"
        android:fontFamily="@font/skybory"
        android:gravity="center"
        android:orientation="horizontal"
        android:padding="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Layout3">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="70dp"
            android:text="로맨스판타지"
            android:textColor="#023e8a"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="53dp"
            tools:layout_editor_absoluteY="467dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="70dp"
            android:text="판타지"
            android:textColor="#023e8a"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="206dp"
            tools:layout_editor_absoluteY="467dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="스포츠"
            android:textColor="#023e8a"
            android:textSize="15dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="317dp"
            tools:layout_editor_absoluteY="467dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/Layout4"
        android:layout_width="407dp"
        android:layout_height="245dp"
        android:descendantFocusability="blocksDescendants"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/includ02">

        <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
            android:id="@+id/youtube_player_view"
            android:layout_width="400dp"
            android:layout_height="wrap_content"
            app:autoPlay="true"
            app:showFullScreenButton="false"
            app:videoId="5uclEvqIUmA" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>